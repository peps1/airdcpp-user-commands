{
  "name": "airdcpp-user-commands",
  "version": "2.1.0-beta.2",
  "description": "User commands to show information in chat",
  "license": "MIT",
  "author": {
    "name": "pepsi"
  },
  "keywords": [
    "airdcpp",
    "airdcpp-extensions",
    "airdcpp-extensions-public"
  ],
  "bugs": {
    "url": "https://github.com/peps1/airdcpp-user-commands/issues/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peps1/airdcpp-user-commands"
  },
  "scripts": {
    "build": "tsc && npm run clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/main.ts",
    "start": "node devtools/watch.js ../dist/main.js",
    "test": "nyc mocha -r ts-node/register src/tests/*.ts",
    "posttest": "nyc report --reporter=json",
    "prepublishOnly": "npm run build",
    "webpack": "webpack"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.14",
    "@types/sinon": "^7.5.2",
    "@types/source-map-support": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^6.0.3",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsdoc": "^26.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-react": "^7.20.3",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^8.1.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "airdcpp-apisocket": "^2.3.1",
    "airdcpp-extension": "^1.2.0",
    "airdcpp-extension-settings": "^1.1.0"
  },
  "main": "dist/main",
  "types": "dist/main",
  "airdcpp": {
    "apiVersion": 1,
    "minApiFeatureLevel": 0
